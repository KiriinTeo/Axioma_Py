{
  "openlibrary": {
    "url": "https://openlibrary.org/search.json",
    "params": ["q", "limit"],
    "requires_key": false,
    "default_response_path": "docs",
    "default_fields": ["title", "author_name", "first_publish_year"]
  },
  "freetogame": {
    "url": "https://www.freetogame.com/api/games",
    "params": [],
    "requires_key": false,
    "default_response_path": "",
    "default_fields": ["title", "genre", "platform", "release_date"]
  },
  "tmdb": {
    "url": "https://api.themoviedb.org/3/search/movie",
    "params": ["api_key", "query", "page", "year"],
    "requires_key": true,
    "default_response_path": "results",
    "default_fields": ["title", "release_date", "overview", "vote_average"]
  }
}
